{
  "name": "example-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./src/*"
  },
  "exports": {
    "./types": {
      "types": "./src/types.d.ts"
    },
    "./worker-configuration": {
      "types": "./worker-configuration.gen.d.ts"
    }
