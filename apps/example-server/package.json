    "test": "tsc --noEmit"
  },
  "dependencies": {
    "hono": "catalog:"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.15",
    "@stzhu/eslint-config": "^0.8.1",
    "@stzhu/tsconfig": "^0.1.0",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "vite": "catalog:",
    "wrangler": "^4.45.0"
  }
